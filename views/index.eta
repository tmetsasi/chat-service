<h1>Prototype program for chat service</h1>
<form method="POST">
  <input type="text" name="sender" />
  <input type="text" name="message" />
  <input type="submit" value="Send a message!" />
</form>
<ul>
  <% it.messages.forEach((msg) => { %>
    <li><%= msg.sender %>: <%= msg.message %></li>
  <% }); %>
</ul>